{
  "name": "family-planner-inc",
  "private": true,
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=9.6.0"
  },
  "type": "module",
  "resolve": {
    "extensions": [
      ".ts",
      ".tsx",
      ".js",
      ".json"
    ]
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:all": "find . -name \"node_modules\" -type d -exec rm -rf {} +",
    "clean:workspaces": "turbo run clean",
    "dev": "turbo watch dev --continue",
    "dev:web": "turbo watch dev -F web...",
    "dev:expo": "turbo watch dev -F expo...",
    "dev:all": "turbo watch dev --filter=\"./apps/**\"",
    "format": "turbo run format --continue -- --cache --cache-location .cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location .cache/.prettiercache",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "typecheck": "turbo run type-check",
    "ui-add": "turbo run ui-add",
    "deploy": "turbo run deploy",
    "deploy:web": "turbo run deploy:web --filter=web",
    "deploy:expo": "turbo run deploy:expo --filter=expo",
    "deploy:all": "turbo run deploy --filter=\"./apps/**\""
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tamagui/adapt": "^1.132.23",
    "@tamagui/animations-react-native": "^1.132.23",
    "@tamagui/button": "^1.132.23",
    "@tamagui/config": "^1.132.23",
    "@tamagui/core": "^1.132.23",
    "@tamagui/font-inter": "^1.132.23",
    "@tamagui/input": "^1.132.23",
    "@tamagui/linear-gradient": "^1.132.23",
    "@tamagui/polyfill-dev": "^1.132.23",
    "@tamagui/select": "^1.132.23",
    "@tamagui/sheet": "^1.132.23",
    "@tamagui/stacks": "^1.132.23",
    "@tamagui/theme-base": "^1.132.23",
    "@turbo/gen": "^2.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "firebase": "^12.2.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "prettier": "^3.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-web": "^0.19.12",
    "tailwindcss": "^4",
    "tamagui": "^1.132.23",
    "turbo": "^2.1.3",
    "typescript": "^5"
  }
}
